{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "build:esm": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "prepare-doc": "rimraf docs && npx typedoc"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "shyft-js",
  "author": "impin2rex",
  "devDependencies": {
    "@types/bs58": "^4.0.1",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.27",
    "rimraf": "^4.1.2",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typedoc": "^0.23.24",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@metaplex/js": "^4.12.0",
    "@solana/wallet-adapter-base": "^0.9.9",
    "@solana/wallet-adapter-react": "^0.15.8",
    "@solana/wallet-adapter-wallets": "^0.17.0",
    "@solana/web3.js": "^1.50.1",
    "axios": "^1.2.6",
    "bs58": "^5.0.0"
  },
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  }
}
