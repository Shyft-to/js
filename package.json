  },
  "homepage": "https://shyft.to",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^28.1.6",
    "babel-jest": "^29.4.1",
    "dotenv": "^16.0.3",
    "jest": "^29.4.1",
    "rimraf": "^4.1.2",
